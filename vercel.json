{
  "version": 2,
  "public": true,
  "buildCommand": "npm run build-for-server-deploy",
  "installCommand": "npm install",
  "outputDirectory": "build",
  "devCommand": "npm run start-server",
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "build/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ]
}
